{

  "manifest_version": 2,
  "name": "span-search",
  "version": "1.0",

  "description": "Navigates to spanishdict definition of a selection via context menu",

  "icons": {
    "48": "icons/icon-48.png",
  "96": "icons/icon-96.png"
  },

  "content_scripts": [
    {
      "matches": [<all_urls>],
      "js": ["spanishdict-search.js"]
    }
  ]

}